                                    <div class="table table-bordered table-striped">

                                        @* -------------------group by Doctor*@

                                        <div ng-show="epiRow.NotDoctorListNew != null && epiRow.NotDoctorListNew.length > 0 ">

                                            <div ng-repeat="cnRowMaster in epiRow.NotDoctorListNew ">

