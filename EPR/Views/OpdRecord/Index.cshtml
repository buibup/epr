@{
    ViewBag.AngularControllerName = "OpdRecordController";


    ViewBag.Title = "OPD Assessment View";
    Layout = "~/Views/Shared/Template.cshtml";
}

@section HeaderTAG{
}

@section ContentHaderTAG{

    <h1 class="title">
        <i class="fa fa-lg fa-fw fa-medkit"></i><span> OPD Assessment View</span>
    </h1>
    <span class="ribbon-button-alignment">
        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 5px 0px 5px;" ng-click="openWindowPrint($.trim($('#epi-active').text()))">Print</span>
    </span>
    <span class="ribbon-button-alignment @*pull-right hidden-print*@">

        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 0px 0px 0px;" ng-click="openWindowChart()">Growth Chart</span>
        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 0px 0px 0px;" ng-click="openWindowSVH()">Document View SVH</span>
        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 0px 0px 0px;" ng-click="openWindow()">Document View SNH</span>
        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 0px 0px 0px;" ng-click="openWindowPatho()">Patho</span>
        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 0px 0px 0px;" ng-click="openWindowGiendo()">GI endo</span>
        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 0px 0px 0px;" ng-click="openWindowVaccine()">Vaccine View</span>
        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 0px 0px 0px;" ng-click="openWindowSQC()">SEC</span>
        <span class="label label-sm label-primary" style="cursor:pointer; font-size: 10px;margin: 0px 0px 0px 0px;" ng-click="openWindowCheckUp()">CheckUp</span>





        <lable>HN:</lable><input ng-model="patientModel.PAPMI_No" ng-init="patientModel.PAPMI_No='@(ViewBag.Hn)'" ng-keypress="($event.which === 13)?SeachPatient():0" type="text" style="width: 85px; font-size:11px; margin-right:4px; text-align:center;" />




        <button ng-click="SeachPatient()" type="button" class="btn btn-success btn-xs">
            <i class="fa fa-refresh fa-spin" ng-show="searching"></i>
            Search
        </button>

    </span>




}

@section MenuTAG{

    <table class="table table-striped menuTable">
        <thead>
            <tr width="50%">
                <th colspan="2">
                    <div class="btn-group">
                        <input type="hidden" ng-model="Temp" name="Temp" ng-init="null">
                        <input type="hidden" ng-model="TempLocation" name="TempLocation" ng-init="null">
                        <input type="hidden" ng-model="currentDoctor" name="currentDoctor" ng-init="null">
                        <input type="hidden" ng-model="currentLocation" name="currentLocation" ng-init="null">
                    </div>
                    <div class="btn-group btn-group-justified" role="group" aria-label="...">
                        <div class="btn-group" role="group">
                            <button type="button" class={{StyleBtnDoctor}} ng-click="QuickFilterDoctor()">Doctor</button>
                        </div>
                        <div class="btn-group" role="group">
                            <button type="button" class={{StyleBtnLocation}} ng-click="QuickFilterLocation()">Location</button>
                        </div>

                    </div>
                </th>
            </tr>

            <tr>
                <th colspan="2" class="smart-form menuFilter">
                    <div class="input">
                        <input type="text" ng-model="search_startDate" name="startDate" placeholder="Select a date" class="form-control datepicker" data-dateformat="dd/mm/yy">
                        <i class="icon-append fa fa-calendar"></i>
                    </div>
                    <div class="input">
                        <input type="text" ng-model="search_endDate" name="endDate" placeholder="Select a date" class="form-control datepicker" data-dateformat="dd/mm/yy">
                        <i class="icon-append fa fa-calendar"></i>
                    </div>
                </th>
            </tr>

            <tr>
                @*	<th style="width:15%">Epi Type</th>
                    <th style="width:15%">Date/Time</th>*@
            </tr>
        </thead>

        <tbody style="
    height: 500px;/* Just for the demo          */
    width:220px;
    overflow-y: auto;/* Trigger vertical scroll    */
    overflow-x: hidden;/* Hide the horizontal scroll */
    display:-webkit-box;">
            <tr class="B" ng-repeat-start="epi in patientModel.episodeList | dateRangefilter:search_startDate:search_endDate |docEPIfilter:currentDoctor |docEpifilterLocaion:currentLocation " ng-class="{'opended':$index === LastOpenedRow}" id="epiRow{{$index}}" ng-click="loadEpisodeMedical(epi);activeLastOpenedRow($index);" tabindex="{{$index}}" ng-keydown="keydown($event,$index)">
                <td style="padding:0px 0px 0px 0px;">{{epi.Episplit}}</td>
                <td colspan="2" style="padding-left: 5px;" ng-click="loadEpisodeMedical(epi)">{{epi.EpiDateString}} {{epi.admTime}}</td>
            </tr>
            <tr ng-show="epi.DoctorChiefComPainList.length > 0" ng-repeat-end ng-show="epi.showPhyList" ng-class="{'opended':$index === LastOpenedRow}" id="epiRow{{$index}}" ng-click="loadEpisodeMedical(epi);activeLastOpenedRow($index);" tabindex="{{$index}}" ng-keydown="keydown($event,$index)">
                <td></td>
                <td colspan="2" style="width:250px">
                    <div ng-repeat="doctor in epi.DoctorChiefComPainList" ng-if="doctor.chiefid<2">{{doctor.chiefComplaintShow}}&nbsp;&nbsp;</div>
                </td>
            </tr>
            <tr ng-hide="patientModel.episodeList.length > 0">
                <td colspan="3">No episode found.</td>
            </tr>
        </tbody>
    </table>

}


<div class="row">
    <div ng-show="episodeTabList.length > 0" class="col-sm-12">

        <div class="jarviswidget" id="wid-id-7" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-sortable="false" role="widget" style="">
            <header role="heading">
                <div class="jarviswidget-ctrls" rol="menu">
                    <a ng-click="closeAllEisodeTabMedical()" href="javascript:void(0);" class="button-icon jarviswidget-delete-btn" title="Close all."><i class="fa fa-times"></i></a>
                </div>
                <ul class="nav nav-tabs pull-left in">

                    <li ng-repeat="epiRow in episodeTabList" ng-class="{'active':epiRow.active}">
                        <a data-toggle="tab" href="#epiTab{{$index}}" ng-class="{'opended':epiRow.ERowid === LastOpenedRow}" ng-click="activeLastOpenedRow(epiRow.ERowid)">
                            <span class="hidden-mobile hidden-tablet" id="EPI-Active">
                                {{epiRow.EpisodeNo}}
                                &nbsp;&nbsp;&nbsp;
                                <i ng-click="MarkEisodeTabMedical(epiRow)" class="fa fa-thumb-tack" ng-class="{'marked': epiRow.marked, 'unmarked': !epiRow.marked}"></i>
                                &nbsp;&nbsp;&nbsp;
                                <i ng-click="closeEisodeTabMedical(epiRow)" class="fa fa-times" ng-class="{'fa-spin':!epiRow.loaded}"></i>
                            </span>
                        </a>
                    </li>
                </ul>
                <span class="jarviswidget-loader"><i class="fa fa-refresh fa-spin"></i></span>
            </header>

            <!-- widget div-->
            <div role="content">
                <!-- widget content -->

                <div class="widget-body">
                    <div class="tab-content" style="min-height:200px;">

                        <!-- TAB BOX -->
                        <div ng-repeat="epiRow in episodeTabList" ng-class="{'active':epiRow.active}" class="tab-pane" id="epiTab{{$index}}">
                            <div ng-show="epiRow.loaded && !epiRow.hasError">



                                <div style="margin: 0 0 20px 0;" class="hidden-print">
                                    <span class="label label-md" ng-class="{'label-warning':epiRow.curDoctorCode=='', 'label-success':epiRow.curDoctorCode!=''}" style=" font-size: 13px; cursor:pointer;" ng-click="filterByDoctor(epiRow,'','')">All Doctor</span>
                                    <span id="doctorList" ng-repeat="doctor in epiRow.DoctorChiefComPainAllList" ng-show="doctor.DoctorCode != null && doctor.DoctorCode !=''   " ng-click="filterByDoctor(epiRow, doctor.DoctorCode, doctor.LocationCode,doctor.DoctorName)" ng-class="{'label-warning':epiRow.curDoctorCode==doctor.DoctorCode  , 'label-success':epiRow.curDoctorCode!=doctor.DoctorCode}" style="cursor:pointer; font-size: 13px;margin: 0px 5px 0px 5px;" class="label label-sm label-success">
                                        {{doctor.DoctorName}}
                                    </span>
                                </div>

                                <div class="table-responsive mw870">
                                    <table class="table table-bordered table-striped" id="PatientDetail">

                                        <tbody>
                                            <tr>
                                                <td class="lF" id="PatientName">Patient Name: &nbsp;  {{epiRow.PAPMIName3}} {{epiRow.PAPMIName}} {{epiRow.PAPMIName2}}</td>
                                                <td class="lF" id="Gender">Gender: &nbsp; {{epiRow.SexCode}}</td>
                                                <td class="lF" id="HN">HN: &nbsp;{{epiRow.PAPMINo}} </td>

                                            </tr>
                                            <tr>
                                                <td class="lF" id="dob">Date of Birth:&nbsp; {{epiRow.DOBDateString}}</td>
                                                <td class="lF" id="Age">Age:&nbsp; {{epiRow.EpiAgeYear}} Years {{epiRow.EpiAgeMonth}} Months</td>
                                                <td class="lF" id="EPI">EN: &nbsp;{{epiRow.EpisodeNo}} &nbsp;&nbsp;  Episode Date/Time: &nbsp;{{epiRow.EpiDateString}} {{epiRow.admTime}}</td>

                                            </tr>

                                        </tbody>
                                    </table>

                                </div>

                                <div class="table-responsive mw870">
                                    <div class="table table-bordered table-striped" id="screening">
                                        <lable class="lF">History of Allergy:</lable>
                                        <table>

                                            <tbody>
                                                <tr ng-repeat="cnRow in epiRow.AllergiesModelList">
                                                    <td style="width:50px;"></td>
                                                    <td> {{cnRow.ALGDescAll}} </td>
                                                </tr>
                                                <tr ng-show="epiRow.AllergiesModelList.length < 1 ">
                                                    <td style="width:50px;"></td>
                                                    <td>- </td>
                                                </tr>
                                            </tbody>
                                        </table>

                                        <lable class="lF" ng-show="epiRow.ObservationAllergyModelList != null && epiRow.ObservationAllergyModelList.length > 0  && epiRow.AgeY <= '15'">Screening of Allergy for this Episode:</lable>
                                        <table ng-show="epiRow.ObservationAllergyModelList != null && epiRow.ObservationAllergyModelList.length > 0  && epiRow.AgeY <= '15' ">

                                            <tbody>
                                                <tr ng-repeat="cnRow in epiRow.ObservationAllergyModelList">
                                                    <td style="width:50px;"></td>
                                                    <td><lable class="lF"> Drug Allergy: </lable></td>
                                                    <td id="ScreeningAllerygy">

                                                        <input type="checkbox" ng-model="Drug" ng-true-value="1" ng-false-value="0" ng-checked="{{cnRow.HxDrug}} == 1" disabled="disabled">
                                                        <label> NKA </label>

                                                        <input type="checkbox" ng-model="Drug" ng-true-value="1" ng-false-value="0" ng-checked="{{cnRow.HxDrugY}} == 1" disabled="disabled">
                                                        <label> YES </label>
                                                        <u>
                                                            &nbsp;
                                                            {{cnRow.HxOtherDesc}} &nbsp;  &nbsp; &nbsp;  &nbsp; &nbsp;  &nbsp;
                                                        </u>
                                                        &nbsp;  &nbsp;&nbsp;  &nbsp;
                                                        <lable class="lF">Food Allergy: </lable>
                                                        <input type="checkbox" ng-model="Food" ng-true-value="1" ng-false-value="0" ng-checked="{{cnRow.HxFood}} == 1" disabled="disabled">

                                                        <label> NKA </label>

                                                        &nbsp;  &nbsp;
                                                        <input type="checkbox" ng-model="Food" ng-true-value="1" ng-false-value="0" ng-checked="{{cnRow.HxFoodY}} == 1" disabled="disabled">

                                                        <label> YES </label>
                                                        <u>
                                                            &nbsp;
                                                            {{cnRow.HxFoodDesc}} &nbsp;  &nbsp; &nbsp;  &nbsp;&nbsp;  &nbsp;
                                                        </u>
                                                        &nbsp;  &nbsp;

                                                        <lable class="lF"> Other: </lable>

                                                        {{cnRow.HxOther}}



                                                    </td>



                                                </tr>
                                            </tbody>
                                        </table>


                                        <lable class="lF" ng-show="epiRow.ObservationModelList != null && epiRow.ObservationModelList.length > 0 && epiRow.chkObs=='Y'">Pain Score & FallRisk:</lable>
                                        <table ng-show="epiRow.ObservationModelList != null && epiRow.ObservationModelList.length > 0 && epiRow.chkObs=='Y'">

                                            <tbody>
                                                <tr ng-repeat="observRow in epiRow.ObservationModelList" id="PainScore">
                                                    <td style="width:50px;"></td>
                                                    <td> Pain Score:  {{observRow.PS}}  &nbsp;&nbsp;&nbsp; Assessed Tool:   {{observRow.AssessTol}}  </td>
                                                    <td>&nbsp;&nbsp;&nbsp;Location:  {{observRow.PAINLOC}}   &nbsp;&nbsp;&nbsp;  </td>
                                                    <td>&nbsp;&nbsp;&nbsp; Duration:  {{observRow.Duration}}  &nbsp;&nbsp;&nbsp;Character of Pain:  {{observRow.CHARAC}}  </td>

                                                </tr>
                                                <tr ng-repeat="observRow in epiRow.ObservationModelList" id="FallRisk">
                                                    <td style="width:50px;"></td>
                                                    <td>FallRisk:{{observRow.FallRisk}} &nbsp;&nbsp;&nbsp; Area of Risk : {{observRow.AreaofRisk}}</td>
                                                    <td></td>
                                                </tr>
                                            </tbody>
                                        </table>

                                        <lable class="lF" ng-show="epiRow.ObservationModelList != null && epiRow.ObservationModelList.length > 0 && epiRow.chkObs=='Y'">Vital Signs:</lable>
                                        <table ng-show="epiRow.ObservationModelList != null && epiRow.ObservationModelList.length > 0 && epiRow.chkObs=='Y'">

                                            <tbody>
                                                <tr ng-repeat="observRow in epiRow.ObservationModelList" id="Temperature">
                                                    <td style="width:50px;"></td>
                                                    <td>Temp:  {{observRow.Temperature}} &#x2103;  &nbsp;&nbsp;&nbsp; PR:  {{observRow.Pulses}} /min   &nbsp;&nbsp;&nbsp; RR: {{observRow.Respiration}} /min  &nbsp;&nbsp;&nbsp; BP:  {{observRow.BPSystolic}}/{{observRow.BPDiastolic}} &nbsp; mm/Hg  &nbsp;&nbsp;&nbsp; O2Sat: {{observRow.OxygenSaturation}} % &nbsp;&nbsp;&nbsp;  BW: {{observRow.weight}}  kgs &nbsp;&nbsp;&nbsp; Ht: {{observRow.height}} cms. &nbsp;&nbsp;&nbsp; BMI: {{observRow.BMI}}   <div ng-show="epiRow.AgeY <= '15'">&nbsp;&nbsp; HC:{{observRow.Head}}  cm  &nbsp;&nbsp; CC:{{observRow.chest}}</div> </td>


                                                </tr>
                                            </tbody>
                                        </table>

                                        <lable class="lF" ng-show="epiRow.ObservationModelList != null && epiRow.ObservationModelList.length > 0 && epiRow.chkObs=='Y'">Triage Level:</lable>
                                        <table ng-show="epiRow.ObservationModelList != null && epiRow.ObservationModelList.length > 0 && epiRow.chkObs=='Y'">

                                            <tbody>
                                                <tr ng-repeat="observRow in epiRow.ObservationModelList" id="Triage">
                                                    <td style="width:50px;"></td>
                                                    <td>Triage Level:  {{observRow.Triage}} </td>
                                                </tr>
                                            </tbody>
                                        </table>








                                    </div>





                                    @*  //------------------------------------------------------------*@



